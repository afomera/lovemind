<h1>Resources</h1>
<% @resource.each do |r| %>
<table class="table table-striped">
<thead>
	<tr>
		<th>Name</th>
		<th>Address</th>
		<th>Notes</th>
	</tr>
</thead>
<tbody>
	<tr>
		<td><%= link_to "#{r.name}", r %></td> <td><%= r.address %></td> <td><%= r.notes %></td>
	</tr>
</tbody>
</table>
<% end %>