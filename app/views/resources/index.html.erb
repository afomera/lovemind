<h4>Resources</h4>
	<div class="pull-right">
		<%= search_form_for @search do |f| %>
			<%= text_field_tag :location, params[:location], placeholder: "Houston, TX" %>
			<%= f.submit %>
		<% end %>
	</div>
<% @resources.each do |r| %>
<table class="table table-striped">
<thead>
	<tr>
		<th>Name</th>
		<th>Address</th>
		<th>Notes</th>
	</tr>
</thead>
<tbody>
	<tr>
		<td><%= link_to "#{r.name}", r %></td> <td><%= r.address %></td> <td><%= r.notes %></td>
	</tr>
</tbody>
</table>
<% end %>
